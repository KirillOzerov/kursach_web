@{
    ViewData["Title"] = "News";
}

<div class="text-center">
    <h1 class="display-4">Новости</h1>
    <ul>
        @foreach (var item in Model)
        {
            <li>
                <span>@item.Title</span>
                <span>@item.Text</span>
                <span>@item.Date.AddHours(3).ToString("yyyy.MM.dd")</span>
            </li>
        }
    </ul>
</div>
