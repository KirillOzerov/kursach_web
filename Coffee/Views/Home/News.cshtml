@{
    ViewData["Title"] = "News";
}

<div class="text-center">
    <h1 class="display-4">News</h1>
    <ul>
        @foreach (var item in Model)
        {
            <li>
                <span>@item.Title</span>
                <span>@item.Text</span>
                <span>@item.Date.ToString("yyyy.MM.dd HH:mm")</span>
            </li>
        }
    </ul>
</div>
